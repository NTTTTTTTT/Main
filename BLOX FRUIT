	if _G.ifnjump then
		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
	end
end)  


spawn(function()
        pcall(function()
            game:GetService("RunService").Stepped:Connect(function()
                  if _G.mi_bbitem or _G.m_b8 or _G.mr_b4 or _G.mr_b5 or _G.mr_b6 or _G.mi_b8 or _G.c_btpprl or _G.m_b2 or _G.m_b7 or _G.mi_bc or _G.t_b3 or _G.m_b3 or _G.m_b4 or _G.mob_b1 or  _G.mob_bb2 or _G.mi_b4 or _G.r_btp1 or _G.r_bb3 or _G.r_b4 or _G.r_btp2 or _G.r_b7 or _G.ef_b6 or _G.ef_b8 or _G.mi_bb1 or _G.mi_b2 or _G.mi_b3 or _G.mi_bb5 or _G.mi_bb6 or _G.t_itp or _G.t_bb7 or _G.mr_b1 or _G.mr_b2 or _G.c_b2 or _G.c_b3 then
