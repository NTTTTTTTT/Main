function itembar()
make_selectscrollingframe(mi_itembar, b_page6,  UDim2.new(0, 0, 0.3100000000, 0))
--end bar core
end

game:GetService('RunService').RenderStepped:connect(function()
if _G.mi_bitem then
checkitem()
for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                                        if v.Name ~= NameItem or v.Name == "Deandre [Lv. 1750]" or v.Name == "Urban [Lv. 1750]" then
                                        if v.Humanoid.Health > 0 then -- check health    
                                               local Distance = (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude         
if Distance >= 250 then                       
                                          local tween =  game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Distance/160), {CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, st_b5.Text, 0) }) tween:Play()                 
                 elseif Distance < 250 then                       
                                          local tween =  game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Distance/500), {CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, st_b5.Text, 0) }) tween:Play()                 
                        end                           
  v.Humanoid:ChangeState(14)
      v.HumanoidRootPart.Size = Vector3.new(50, 50, 10)     --size hix box  
      v.HumanoidRootPart.CanCollide = false                                                
v.Head.CanCollide = false                                         
       if (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 65 then
  click() -- click  
    usetool()    
end end  end end 
if game:GetService("ReplicatedStorage"):FindFirstChild(NameItem) or game:GetService("WorkSpace")["Enemies"]:FindFirstChild(NameItem) then
_G.mi_bbitem = true
else
_G.mi_bbitem = false
end
 end 
if mi_bitem.Text == "" then
_G.mi_bbitem = false
end
end)


spawn(function() 
    while task.wait(3) do   
    if _G.mi_bitem then
    checkitem()
    if game:GetService("ReplicatedStorage"):FindFirstChild(NameItem) then        
                               local Distance = (game:GetService("ReplicatedStorage"):FindFirstChild(NameItem).HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                                        local tween =  game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Distance/160), {CFrame = game:GetService("ReplicatedStorage"):FindFirstChild(NameItem).HumanoidRootPart.CFrame * CFrame.new(0, st_b5.Text, 0) }) tween:Play()                                                            
     end end end end)                        

