-- // Auto God Human

function checkgodhuman()
if game.PlaceId == 7449423635 then -- sea3                   
 for i,v in pairs(game.ReplicatedStorage.Remotes.CommF_:InvokeServer("getInventory")) do
								if v.Name == "Dragon Scale" then
									if v.Count <= 10 then
TPGodhuman=CFrame.new(6594,383,139)
MobGodhuman="v.Name == 'Fishman Raider [Lv. 1775]' or v.Name == 'Fishman Captain [Lv. 1800]'"
end end
if v.Name == "Fish Tail" then
									if v.Count <= 20 then
   TPGodhuman = CFrame.new(-10993,332,-8940)             
MobGodhuman= "v.Name == 'Fishman Raider [Lv. 1775]' or v.Name == 'Fishman Captain [Lv. 1800]'"
end end end end
if game.PlaceId == 4442272183 then -- sea2
for i,v in pairs(game.ReplicatedStorage.Remotes.CommF_:InvokeServer("getInventory")) do
								if v.Name == "Magma Ore" then
									if v.Count <= 20 then
TPGodhuman=CFrame.new(-5428,78,-5959)
MobGodhuman=  "v.Name == 'Magma Ninja [Lv. 1175]'"
end end
if v.Name == "Mystic Droplet" then
									if v.Count <= 10 then
     TPGodhuman=CFrame.new(-3385,239,-10542)
     MobGodhuman="v.Name == 'Water Fighter [Lv. 1450]'"
     end end end end
end

game:GetService('RunService').RenderStepped:connect(function()
if _G.mi_b8 then
checkgodhuman()
if game.Players.LocalPlayer.Backpack:FindFirstChild("Godhuman") then                  
                    _G.mi_b8 = false
   end
   if not game.Players.LocalPlayer.Backpack:FindFirstChild("Godhuman") then           
      game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuyGodhuman")                                                                                                              
       if _G.mi_b8 then                    
                        TP1(TPGodhuman)
   end                     
                        for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
                        if MobGodhuman then                       
                        local Distance = (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                  if Distance < 300 then
                  Speed =500          
    elseif Distance >= 300 then
                  Speed = 160
           end          
           if _G.mi_b8 then
       if v.Humanoid.Health > 0 then -- check health
     usetool()
     click() -- click          
       v.Humanoid:ChangeState(14)
      v.HumanoidRootPart.Size = Vector3.new(50, 50, 10)     --size hix box  
      v.HumanoidRootPart.CanCollide = false                                                
v.Head.CanCollide = false                                 
if v.Humanoid:FindFirstChild("Animator") then
                            v.Humanoid.Animator:Destroy()
                        end                        
game:GetService("TweenService"):Create(
                        game.Players.LocalPlayer.Character.HumanoidRootPart,
                        TweenInfo.new(Distance/Speed, Enum.EasingStyle.Linear),
                        {CFrame = v.HumanoidRootPart.CFrame* CFrame.new(0, st_b5.Text, 0) }
                    ):Play()       
 end end end end end end end)

