make_textlabel(mi_tboss, b_page6, "Farm Boss", UDim2.new(0, 0, 0.0300000000, 0))

make_select2(mi_f1,mi_bossb, mi_b1 , b_page6, "Boss : Select", UDim2.new(0, 0, 0.1000000, 0))
mi_bossb.MouseButton1Down:connect(function()
mi_bossbar.Visible = true
end)

mi_b1.MouseButton1Down:connect(function()
--on off 
if mi_b1.Text == "" then --on
mi_b1.Text = "X"
_G.mi_b1 = true
else
_G.mi_b1 = false
mi_b1.Text = ""
stoptp()
end end)

game:GetService('RunService').RenderStepped:connect(function()
if _G.mi_b1 then
checkboss()
for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                                        if v.Name ~= NameBoss or v.Name == "Deandre [Lv. 1750]" or v.Name == "Urban [Lv. 1750]" then
                                        if v.Humanoid.Health > 0 then -- check health                                            
                                               local Distance = (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude                                
                                               if Distance >= 250 then
                                          local tween =  game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Distance/160), {CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, st_b5.Text, 0) }) tween:Play()                 
                                          elseif Distance < 250 then
                                          local tween =  game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Distance/500), {CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, st_b5.Text, 0) }) tween:Play()                 
                                          end
                                          v.Humanoid:ChangeState(14)
      v.HumanoidRootPart.Size = Vector3.new(50, 50, 10)     --size hix box  
      v.HumanoidRootPart.CanCollide = false                                                
v.Head.CanCollide = false                                         
       if (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 65 then
  click() -- click  
    usetool()    
end end  end end
if game:GetService("ReplicatedStorage"):FindFirstChild(NameBoss) or game:GetService("WorkSpace")["Enemies"]:FindFirstChild(NameBoss) then
_G.mi_bb1 = true
else
_G.mi_bb1 = false
end
 end 
if mi_b1.Text == "" then
_G.mi_bb1 = false
end
end)


spawn(function() 
    while task.wait(3) do   
    if _G.mi_b1 then
    checkboss()
    if game:GetService("ReplicatedStorage"):FindFirstChild(NameBoss) then        
                               local Distance = (game:GetService("ReplicatedStorage"):FindFirstChild(NameBoss).HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                                        local tween =  game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Distance/160), {CFrame = game:GetService("ReplicatedStorage"):FindFirstChild(NameBoss).HumanoidRootPart.CFrame * CFrame.new(0, st_b5.Text, 0) }) tween:Play()                                                            
     end end end end)                                   

