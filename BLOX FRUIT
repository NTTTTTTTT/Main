function Chestkeycup()                               
      if game:GetService("Workspace"):FindFirstChild("Chest3") then
for i,v in pairs(game.Workspace:GetChildren()) do
             if string.find(v.Name, "Chest3") then
             local pos = v --check pos             
                    local Distance = (pos.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude                 
    if Distance >= 0 then
                  Speed = 1000000
           end              
      if _G.mi_bc then                                 
           game:GetService("TweenService"):Create(
                        game.Players.LocalPlayer.Character.HumanoidRootPart,
                        TweenInfo.new(Distance/Speed, Enum.EasingStyle.Linear),
                        {CFrame =v.CFrame}
                    ):Play()    
end end  end             
end
                                                        if not game:GetService("Workspace"):FindFirstChild("Chest3") then
             for i,v in pairs(game.Workspace:GetChildren()) do
             if string.find(v.Name, "Chest2") then
             local pos = v --check pos             
                    local Distance = (pos.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude                 
    if Distance >= 0 then
                  Speed = 1000000
           end                       
        if _G.mi_bc then                
           game:GetService("TweenService"):Create(
                        game.Players.LocalPlayer.Character.HumanoidRootPart,
                        TweenInfo.new(Distance/Speed, Enum.EasingStyle.Linear),
                        {CFrame =v.CFrame}
                    ):Play()    
     end  end         end
end

if not game:GetService("Workspace"):FindFirstChild("Chest2") then
             for i,v in pairs(game.Workspace:GetChildren()) do
             if string.find(v.Name, "Chest1") then
             local pos = v --check pos             
                    local Distance = (pos.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude                 
    if Distance >= 0 then
                  Speed = 1000000
           end                                           
             if _G.mi_bc then    
     game:GetService("TweenService"):Create(
                        game.Players.LocalPlayer.Character.HumanoidRootPart,
                        TweenInfo.new(Distance/Speed, Enum.EasingStyle.Linear),
                        {CFrame =v.CFrame}
                    ):Play()    
end end  end
end          

end

if game.PlaceId == 2753915549 then -- sea1
mi_fc.Text = "   Sea 1 Can't Find Fist of Darkness - God'd Chalice"
end
if game.PlaceId == 4442272183 then -- sea2
mi_fc.Text = "   Auto Find Fist of Darkness"
end
if game.PlaceId == 7449423635 then -- sea3
mi_fc.Text = "   Auto God's Chalice"
end

if game.PlaceId == 4442272183 then -- sea2

function chestsea2(x)
for i,v in pairs(game:GetService("ReplicatedStorage").Unloaded:GetChildren()) do
             if string.find(v.Name, x) then
        
             game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=v.Chest1.CFrame           
             game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=v.Chest2.CFrame                                           
             game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame= v.Chest3.CFrame                                         
            
end end end

game:GetService('RunService').RenderStepped:connect(function()       
     if _G.mi_bc then    
    if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Fist of Darkness") then 
    _G.mi_bc = false   
    mi_bc.Text = ""
end end end) 

game:GetService('RunService').RenderStepped:connect(function()       
     if _G.mi_bc then    
Chestkeycup()
end end)
--[[
game:GetService('RunService').RenderStepped:connect(function()       
     if _G.mi_bc then    
chestsea2("Chests")
end end)
